PV_COUNT      = 1;
PV_MASK       = DBE_VALUE;

PV_SUFFIX_AMPW = Aset:WR;
PV_CHTYPE      = DBR_LONG;
PV_COUNT       = 1;
PV_MASK        = DBE_VALUE;


OBJECTS_START;

NUMBER_OF_OBJECTS = 1;      For sanity checks
NUMBER_OF_ILOCKS  = 0;      If the number of interlocks changes between objects, then redefine this before the "name" paramater

NAME    = GUN-LLRF;
PV_ROOT = EBT:GUN:;
LLRF_MAX_AMPLITUDE = 2000; maximum amplitude that the controller can apply, for additionaly saftey this could be compiled in at source...   

